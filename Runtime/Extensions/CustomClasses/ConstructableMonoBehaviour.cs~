using UnityEngine;

namespace Francespo.Extensions
{
    public abstract class ConstructableMonoBehaviour<A0> : MonoBehaviour { public abstract void Init(A0 arg0); }
    public abstract class ConstructableMonoBehaviour<A0, A1> : MonoBehaviour { public abstract void Init(A0 arg0, A1 arg1); }
    public abstract class ConstructableMonoBehaviour<A0, A1, A2> : MonoBehaviour { public abstract void Init(A0 arg0, A1 arg1, A2 arg2); }
    public abstract class ConstructableMonoBehaviour<A0, A1, A2, A3> : MonoBehaviour { public abstract void Init(A0 arg0, A1 arg1, A2 arg2, A3 arg3); }
    public abstract class ConstructableMonoBehaviour<A0, A1, A2, A3, A4> : MonoBehaviour { public abstract void Init(A0 arg0, A1 arg1, A2 arg2, A3 arg3, A4 arg4); }
    public abstract class ConstructableMonoBehaviour<A0, A1, A2, A3, A4, A5> : MonoBehaviour { public abstract void Init(A0 arg0, A1 arg1, A2 arg2, A3 arg3, A4 arg4, A5 arg5); }
    public abstract class ConstructableMonoBehaviour<A0, A1, A2, A3, A4, A5, A6> : MonoBehaviour { public abstract void Init(A0 arg0, A1 arg1, A2 arg2, A3 arg3, A4 arg4, A5 arg5, A6 arg6); }
    public abstract class ConstructableMonoBehaviour<A0, A1, A2, A3, A4, A5, A6, A7> : MonoBehaviour { public abstract void Init(A0 arg0, A1 arg1, A2 arg2, A3 arg3, A4 arg4, A5 arg5, A6 arg6, A7 arg7); }
    public abstract class ConstructableMonoBehaviour<A0, A1, A2, A3, A4, A5, A6, A7, A8> : MonoBehaviour { public abstract void Init(A0 arg0, A1 arg1, A2 arg2, A3 arg3, A4 arg4, A5 arg5, A6 arg6, A7 arg7, A8 arg8); }
}
